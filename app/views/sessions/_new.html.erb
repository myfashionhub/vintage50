<div class="login">
    <h3>
      Log in below or <a href="/users/new">Sign up</a>
    </h3>

    <%= form_tag sessions_path, method: :post do %>
      <%= text_field_tag :email, nil, placeholder: "Email" %>
      <%= password_field_tag :password, nil, placeholder: "Password" %>
      <%= submit_tag "Log in" %>
    <% end %>
  </div>
</div>
